      "light": "bright_blue"
    },
    "textMuted": {
      "dark": "light0",
      "light": "light0"
    },
    "background": {
      "dark": "dark0",
      "light": "dark0"
    },
    "backgroundPanel": {
      "dark": "dark1",
      "light": "dark1"
    },
    "backgroundElement": {
      "dark": "dark2",
      "light": "dark2"
    },
    "border": {
      "dark": "dark3",
      "light": "dark3"
    },
