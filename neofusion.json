    "markdownCodeBlock": {
      "dark": "bright_blue",
      "light": "bright_blue"
    },
    "syntaxComment": {
      "dark": "neutral_blue",
      "light": "neutral_blue"
    },
    "syntaxKeyword": {
      "dark": "bright_red",
      "light": "bright_red"
    },
    "syntaxFunction": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "syntaxVariable": {
      "dark": "bright_orange",
      "light": "bright_orange"
    },
    "syntaxString": {
      "dark": "bright_orange",
      "light": "bright_orange"
    },
    "syntaxNumber": {
      "dark": "neutral_purple",
      "light": "neutral_purple"
    },
    "syntaxType": {
      "dark": "bright_purple",
      "light": "bright_purple"
    },
    "syntaxOperator": {
      "dark": "bright_orange",
      "light": "bright_orange"
    },
    "syntaxPunctuation": {
      "dark": "bright_blue",
      "light": "bright_blue"
    }
