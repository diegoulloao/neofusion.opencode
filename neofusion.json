    "markdownLinkText": {
      "dark": "neutral_purple",
      "light": "neutral_purple"
    },
    "markdownCode": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "markdownBlockQuote": {
      "dark": "neutral_blue",
      "light": "neutral_blue"
    },
    "markdownEmph": {
      "dark": "bright_purple",
      "light": "bright_purple"
    },
    "markdownStrong": {
      "dark": "light0",
      "light": "light0"
