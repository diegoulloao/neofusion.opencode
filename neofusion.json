{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "dark0": "#06101e",
    "dark1": "#031B26",
    "dark2": "#052839",
    "dark3": "#06364B",
    "dark4": "#08435E",
    "light0": "#66def9",
    "light1": "#66def9",
    "light2": "#008DA3",
    "light3": "#006A7A",
    "light4": "#004752",
    "bright_red": "#fd5e3a",
    "bright_green": "#35b5ff",
    "bright_yellow": "#fd5e3a",
    "bright_blue": "#e2d9c5",
    "bright_purple": "#fd5e3a",
    "bright_aqua": "#e2d9c5",
    "bright_orange": "#fd5e3a",
    "neutral_blue": "#22536f",
    "neutral_purple": "#ec30ac",
    "faded_green": "#5f6e29",
    "dark_red": "#722529",
    "dark_green": "#5f6e29",
    "dark_aqua": "#06364B"
