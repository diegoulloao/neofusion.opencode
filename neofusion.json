      "dark": "light0",
      "light": "light2"
    },
    "secondary": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "accent": {
      "dark": "bright_orange",
      "light": "bright_orange"
    },
    "error": {
      "dark": "bright_red",
      "light": "bright_red"
    },
    "warning": {
      "dark": "bright_purple",
      "light": "bright_purple"
    },
    "success": {
      "dark": "bright_green",
      "light": "bright_green"
    },
    "info": {
      "dark": "light2",
      "light": "light2"
